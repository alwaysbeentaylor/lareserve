PORT=3001
DATABASE_PATH=./data/database.sqlite

# AI Integration
OPENAI_API_KEY=your_openai_api_key_here

# Search APIs
BRAVE_SEARCH_API_KEY=your_brave_api_key_here
SCRAPER_API_KEY=your_scraper_api_key_here

# Google Search Configuration
# 2Captcha for Google reCAPTCHA solving (required for Google Search)
TWO_CAPTCHA_API_KEY=your_2captcha_api_key_here

# Proxy for Google Search requests (optional, format: http://user:pass@host:port)
# Using a proxy helps avoid rate limiting and CAPTCHA challenges
PROXY_URL=

# Google Search rate limiting (milliseconds between requests, default: 5000)
GOOGLE_SEARCH_DELAY=5000

# Google Knowledge Graph (for celebrity detection - optional, GPT fallback available)
GOOGLE_KNOWLEDGE_GRAPH_API_KEY=your_google_kg_key_here
